{
  "name": "prophet-kacou-desktop-v2",
  "private": true,
  "version": "1.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:windows-build-signe": "powershell -ExecutionPolicy Bypass -File ./build-signed.ps1",
    "build:x64": "pnpm tauri build --runner cargo-xwin --target x86_64-pc-windows-msvc",
    "build:x32": "pnpm tauri build --runner cargo-xwin --target i686-pc-windows-msvc",
    "build:arm64": "pnpm tauri build --runner cargo-xwin --target aarch64-pc-windows-msvc",
    "mac:build": "pnpm run tauri build -- --target universal-apple-darwin",
    "mac:clean-quarantine": "xattr -rc \"src-tauri/target/universal-apple-darwin/release/bundle/macos/Prophet Kacou.app\"",
    "mac:lonline:sign-app": "codesign --sign \"$APPLE_DISTRIBUTION_CERT_NAME\" --entitlements \"$PATH_TO_ENTITLEMENTS_FILE\" \"src-tauri/target/universal-apple-darwin/release/bundle/macos/$PKP_MACOS_APP_NAME.app\"",
    "mac:online:deploy": "xcrun productbuild --sign \"$MAC_INSTALLER_CERT_NAME\" --component \"src-tauri/target/universal-apple-darwin/release/bundle/macos/$PKP_MACOS_APP_NAME.app\" \"/Applications\" \"$PKP_MACOS_APP_NAME.pkg\"",
    "mac:online-ssh-deploy": "./ssh/mac-online-deploy.sh"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-pdf/renderer": "^4.3.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-router": "^1.121.27",
    "@tanstack/react-router-devtools": "^1.121.27",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "^2.3.1",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-sql": "~2.2.1",
    "@tauri-apps/plugin-updater": "~2",
    "@types/i18next": "^13.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "htmlparser2": "^10.0.0",
    "i18next": "^25.2.1",
    "lucide-react": "^0.522.0",
    "print-js": "^1.6.0",
    "react": "^18.3.1",
    "react-confirm-alert": "^3.0.6",
    "react-dom": "^18.3.1",
    "react-h5-audio-player": "^3.10.0",
    "react-youtube": "^10.1.0",
    "slug": "^11.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "use-debounce": "^10.0.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.121.29",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.0.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/slug": "^5.0.9",
    "@vitejs/plugin-react": "^4.3.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-i18next-loader": "^3.1.2"
  }
}
